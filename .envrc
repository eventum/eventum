layout php
layout node

export PHP_VERSION=7.4

# Use Node version compatible with Travis.
# Pre-installed Node.js versions on Travis (xenial)
# - v4.9.1
# - v6.17.0
# - v8.9
# - v8.12.0
# - v8.15.1
# - v10.15.3
# - v11.0.0
export PATH="/usr/local/opt/node@10/bin:$PATH"

[[ -f .envrc.local ]] && source_env .envrc.local
